cm:
	${MAKE} aim TB=cm

pltro:
	${plt} cm.csv Frequency "vm(ro1),vm(ro2),vm(ro3)" same

pltvg:
	${plt} cm.csv v2 "v(vg1),v(vg2),v(vg3),v(vg4)" "same${OPT}"

