pltid:
	${plt} cm.csv v2 "i(vd1),i(vd2),i(vd3)" "same${OPT}"


aim:
	-rm ${TB}.log
	-rm ${TB}.csv
	aimspice ${TB}.cir -o csv | tee ${TB}.sim
	@cat ${TB}.sim | perl -ne "if(m/error/i){system('cat ${TB}.log'); exit 1;}"

nmos:
	${MAKE} aim TB=nmos_vd same
	
pltnmos:
	${plt} nmos_vd.csv v2 "vm(gain)" same

clean:
	-rm *.csv
	-rm *.log
	-rm *.sim

